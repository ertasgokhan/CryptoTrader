@model List<AppUser>

@{
    ViewData["Title"] = "Admin Page";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<table class="table table-bordered table-responsive-sm table-striped">
    <tr>
        <th>
            User Id
        </th>
        <th>
            User Name
        </th>
        <th>
            Email
        </th>
    </tr>
    @if (Model.Count() == 0)
    {
        <tr>
            <td colspan="3">No Data</td>
        </tr>
    }
    else
    {
        foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.UserName
                </td>
                <td>
                    @item.Email
                </td>
            </tr>
        }
    }
</table>
